{
  "name": "api",
  "version": "0.0.1",
  "description": "Integration med fastAPI.",
  "main": "./index.js",
  "scripts": {
    "migrate": "node-pg-migrate",
    "migrate:up": "node-pg-migrate up",
    "start": "node-pg-migrate up && ts-node -r tsconfig-paths/register src/server.ts",
    "dev": "node-pg-migrate up && ts-node-dev --no-notify --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
    "lint": "eslint ./src/**/*.ts",
    "prod": "tsc && node ./dist/lib/server.js",
    "test": "npm run test:watch",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:Iteam1337/svensk-allmannytta.git"
  },
  "author": "Anders, Martin",
  "license": "ISC",
  "dependencies": {
    "@iteam/config": "12.1.0",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "node-pg-migrate": "4.6.2",
    "pg": "8.0.3",
    "sql-template-strings": "2.2.2",
    "xml2js": "0.4.23",
    "xml2json": "0.12.0"
  },
  "devDependencies": {
    "@iteam/eslint-config-node": "0.3.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.4",
    "@types/jest": "25.2.1",
    "@types/pg": "7.14.3",
    "@types/xml2json": "0.11.0",
    "@types/xml2js": "0.4.5",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "dotenv": "8.2.0",
    "eslint": "5.9.0",
    "jest": "25.2.7",
    "jest-watch-typeahead": "0.5.0",
    "prettier": "2.0.4",
    "ts-jest": "25.3.1",
    "ts-node": "8.8.2",
    "ts-node-dev": "1.0.0-pre.44",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
