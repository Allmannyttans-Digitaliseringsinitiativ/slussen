{
  "name": "api",
  "version": "0.0.3",
  "description": "Integration med fastAPI.",
  "main": "./index.js",
  "scripts": {
    "build": "tsc",
    "dev": "node-pg-migrate up && ts-node-dev --no-notify --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
    "docs": "node generateSwagger",
    "lint": "eslint ./src/**/*.ts",
    "migrate:up": "node-pg-migrate up",
    "migrate:down": "node-pg-migrate down",
    "migrate": "node-pg-migrate -f migrate-config.json",
    "prod": "tsc && node ./dist/lib/server.js",
    "start": "node-pg-migrate up && ts-node -r tsconfig-paths/register src/server.ts",
    "test:ci": "jest --ci --forceExit",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test": "npm run test:watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Allmannyttans-Digitaliseringsinitiativ/api.git"
  },
  "author": "Anders, Martin",
  "license": "ISC",
  "dependencies": {
    "@iteam/config": "12.1.0",
    "@types/http-errors": "^1.6.3",
    "@types/jsonwebtoken": "^8.5.0",
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "express-async-handler": "1.1.4",
    "jsonwebtoken": "^8.5.1",
    "node-pg-migrate": "4.6.2",
    "pg": "8.0.3",
    "sql-template-strings": "^2.2.2",
    "swagger-ui-express": "^4.1.4",
    "typescript": "^3.8.3",
    "xml2js": "0.4.23",
    "xml2json": "0.12.0"
  },
  "devDependencies": {
    "@iteam/eslint-config-node": "0.3.0",
    "@types/cors": "2.8.6",
    "@types/express": "4.17.4",
    "@types/jest": "25.2.1",
    "@types/pg": "7.14.3",
    "@types/supertest": "^2.0.9",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/xml2js": "0.4.5",
    "@types/xml2json": "0.11.0",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "dotenv": "8.2.0",
    "eslint": "5.9.0",
    "jest": "25.2.7",
    "jest-watch-typeahead": "0.5.0",
    "prettier": "2.0.4",
    "supertest": "^4.0.2",
    "swagger-jsdoc": "^4.0.0",
    "ts-jest": "25.3.1",
    "ts-node": "8.8.2",
    "ts-node-dev": "1.0.0-pre.44",
    "ts-to-json": "^0.65.1",
    "tsconfig-paths": "3.9.0"
  },
  "keywords": []
}
